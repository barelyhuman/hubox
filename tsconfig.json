{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
